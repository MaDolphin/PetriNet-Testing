pntest Test02 {
    use petrinet CookieMachine_modified
    testcase ATestCase {
        initial marking {
            Keksspeicher 4,
            Einwurfschlitz 0,

            EinwurfMoeglich 1,
            KeinSignal 1,
            Strom 1
        }

        expect all {
            marking {
                Keksspeicher 4,
                Einwurfschlitz 0
            }
        }

        simulate {
            MuenzeEinwerfen,
            MuenzeAkzeptieren,
            KeksDruecken,
            KekseAusgeben,
            KeksGeben,
            SchachtelEntnehmen
        }

        expect all {
            marking {
                Keksspeicher 3,
                Einwurfschlitz 0
            }
        }

    }
}
